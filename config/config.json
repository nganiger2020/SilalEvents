{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "events-summary-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/eventsSummary/EventsSummaryWebPart.js",
          "manifest": "./src/webparts/eventsSummary/EventsSummaryWebPart.manifest.json"
        }
      ]
    },
    "event-detail-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/eventDetail/EventDetailWebPart.js",
          "manifest": "./src/webparts/eventDetail/EventDetailWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "EventsSummaryWebPartStrings": "lib/webparts/eventsSummary/loc/{locale}.js",
    "EventDetailWebPartStrings": "lib/webparts/eventDetail/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}